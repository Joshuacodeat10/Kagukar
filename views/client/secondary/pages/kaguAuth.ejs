<body data-reg-findable-address="false" data-show-hybrid-css="False" data-is-desktop="true">


<%- include('../partials/common/modal-process')%>

 <div>
      


        <header class="su-Branding ">
         
            <h1 class="su-Branding_Title">Update Details</h1>
 
        </header>


        <div>
          
            <form class="su-Signup">
                <input name="_csrf" type="hidden"
                    value="<%=csrfToken%>" />
                

                <div id="Site">
                    <fieldset data-section="personal-information" class="switchable-area"
                        id="PersonalInformationSection">
                        <legend>Personal information</legend>

                        

                        <div class="su-fieldset_Bordered">


                            <div>
                                <div>
                                    <input autocorrect="off" class="flexiField_Input" data-val="true"
                                        data-val-characterlimit="Please enter your first name."
                                        data-val-characterlimit-isoptional="False"
                                        data-val-characterlimit-maxlength="50" data-val-characterlimit-maxuserinput="50"
                                        data-val-characterlimit-minlength="1" data-val-characterlimit-onblur=""
                                        data-val-regex="Please enter valid characters only."
                                        data-val-regex-expression="^[0-9\ \u00B4\u0041-\u005A\u005F\u0061-\u007A\u00C0-\u0233\u0250-\u02AD\u1E00-\u1EF9\uFB00-\uFB06\uFF20-\uFF2A\uFF41-\uFF4A\u002D\u002E]*$"
                                        data-val-regex-onblur="" data-val-regex-options=""
                                        data-val-required="Please enter your first name." data-val-required-onblur=""
                                        id="FirstName" name="name" spellcheck="false" type="text" value="<%=user.name%>" />
                                    <span class="flexiField_Label">Full Name</span>

                                    <div></div>
                                    <span class="flexiField_AditionalInfo"></span>
                                </div>
                            </div>


                             <div>
                                 <div>
                                     <input autocorrect="off" class="flexiField_Input" data-val="true"
                                         data-val-characterlimit="Please enter your first name."
                                         data-val-characterlimit-isoptional="False"
                                         data-val-characterlimit-maxlength="50"
                                         data-val-characterlimit-maxuserinput="50" data-val-characterlimit-minlength="1"
                                         data-val-characterlimit-onblur=""
                                         data-val-regex="Please enter valid characters only."
                                         data-val-regex-expression="^[0-9\ \u00B4\u0041-\u005A\u005F\u0061-\u007A\u00C0-\u0233\u0250-\u02AD\u1E00-\u1EF9\uFB00-\uFB06\uFF20-\uFF2A\uFF41-\uFF4A\u002D\u002E]*$"
                                         data-val-regex-onblur="" data-val-regex-options=""
                                         data-val-required="Please enter your first name." data-val-required-onblur=""
                                         id="schoolName" name="department" spellcheck="false" type="text"
                                         value="<%=user.department%>" />
                                     <span class="flexiField_Label">School</span>

                                     <div></div>
                                     <span class="flexiField_AditionalInfo"></span>
                                 </div>
                             </div>

                              <div>
                                  <div>
                                      <input autocorrect="off" class="flexiField_Input" data-val="true"
                                          data-val-characterlimit="Please enter your first name."
                                          data-val-characterlimit-isoptional="False"
                                          data-val-characterlimit-maxlength="50"
                                          data-val-characterlimit-maxuserinput="50"
                                          data-val-characterlimit-minlength="1" data-val-characterlimit-onblur=""
                                          data-val-regex="Please enter valid characters only."
                                          data-val-regex-expression="^[0-9\ \u00B4\u0041-\u005A\u005F\u0061-\u007A\u00C0-\u0233\u0250-\u02AD\u1E00-\u1EF9\uFB00-\uFB06\uFF20-\uFF2A\uFF41-\uFF4A\u002D\u002E]*$"
                                          data-val-regex-onblur="" data-val-regex-options=""
                                          data-val-required="Please enter your first name." data-val-required-onblur=""
                                          id="className" name="rank" spellcheck="false" type="text"
                                          value="<%=user.rank%>" placeholder="JSS 1 ... SSS 3" />
                                      <span class="flexiField_Label">Class</span>

                                      <div></div>
                                      <span class="flexiField_AditionalInfo"></span>
                                  </div>
                              </div>
                            

                        </div>

                        
                    </fieldset>
                       

                        
                        

                        

                       

                    <fieldset data-section="contact-information">
                        <legend>Contact information</legend>

                        <div class="su-fieldset_Bordered">


                            <div>
                                <div>
                                    <input autocorrect="off" class="flexiField_Input" data-val="true"
                                        data-val-characterlimit="Please enter a valid email address."
                                        data-val-characterlimit-isoptional="False"
                                        data-val-characterlimit-maxlength="100"
                                        data-val-characterlimit-maxuserinput="100" data-val-characterlimit-minlength="0"
                                        data-val-characterlimit-onblur=""
                                        data-val-regex="Please enter a valid email address."
                                        data-val-regex-expression="^(?=.{6,})([a-zA-Z0-9&#39;‘’_\-\.+]+)@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.)|(([a-zA-Z0-9\-]+\.)+))([a-zA-Z]+|[0-9]{1,3})$"
                                        data-val-regex-onblur="" data-val-regex-options=""
                                        data-val-required="Please enter a valid email address."
                                        data-val-required-onblur="" id="EmailAddress" name=""
                                        spellcheck="false" type="email" value="<%=user.username%>" readonly/>
                                    <span class="flexiField_Label">Email address</span>

                                    <div></div>

                                    <span class="flexiField_AditionalInfo"></span>
                                    <input id="InformationText" name="InformationText" type="hidden"
                                        value="[&quot;Your email address will be used to notify you about important account information.&quot;]" />
                                </div>
                            </div>



                            <div id="ContactNumberArea">


                                <div data-control="phone-number">
                                    <div>
                                        <div class="">
                                            <input autocorrect="off" data-val="true"
                                                data-val-characterlimit="Please enter a valid contact number."
                                                data-val-characterlimit-isoptional="False"
                                                data-val-characterlimit-maxlength="5"
                                                data-val-characterlimit-maxuserinput="5"
                                                data-val-characterlimit-minlength="0" data-val-characterlimit-onblur=""
                                                data-val-regex="Please enter a valid contact number."
                                                data-val-regex-expression="^\+{0,1}(\d){1,}$" data-val-regex-onblur=""
                                                data-val-regex-options=""
                                                data-val-required="Please enter your contact number."
                                                data-val-required-onblur="" id="DialingCode" name="DialingCode"
                                                spellcheck="false" type="tel" value="+234" />

                                            <div></div>

                                            <input autocorrect="off" class="flexiField_Input" data-val="true"
                                                data-val-characterlimit="Please enter a valid contact number."
                                                data-val-characterlimit-isoptional="False"
                                                data-val-characterlimit-maxlength="28"
                                                data-val-characterlimit-maxuserinput="28"
                                                data-val-characterlimit-minlength="4" data-val-characterlimit-onblur=""
                                                data-val-regex="Please enter a valid contact number."
                                                data-val-regex-expression="^([\d\ \-]){3,}[\d][\ \-]*$"
                                                data-val-regex-onblur="" data-val-regex-options=""
                                                data-val-remote="The number you have provided does not appear to be a mobile number."
                                                data-val-remote-action="ValidateMobileNumber" data-val-remote-onblur=""
                                                data-val-remote-url="hdnValidateMobileNumber"
                                                data-val-remote-validation="The number you have provided does not appear to be a mobile number."
                                                data-val-required="Please enter your contact number."
                                                data-val-required-onblur="" id="PhoneNumber" name="PhoneNumber"
                                                spellcheck="false" type="tel" value="" />

                                            <div></div>

                                            <input id="InformationText" name="InformationText" type="hidden"
                                                value="[&quot;Your contact number is needed in case we need to speak to you about your account.&quot;]" />
                                        </div>
                                        <span class="flexiField_Label">Contact number</span>
                                    </div>
                                </div>
                                <input id="GdprIso2Char" type="hidden" value="NG" />
                            </div>
                            <div class="clear"></div>
                        </div>
                    </fieldset>

                    
                    
               


                
                    <fieldset data-section="user-preferences">
                        <legend>About</legend>
                        <div id="RealityCheckArea"> </div>

                        <div id="BonusCodeArea">
                            <div class="su-fieldset_Bordered">


                                <div>
                                    <div>
                                        <textarea autocorrect="off" class="flexiField_Input" data-val="true"
                                            data-val-characterlimit="The incorrect number of characters have been entered."
                                            data-val-characterlimit-isoptional="True"
                                            data-val-characterlimit-maxlength="15"
                                            data-val-characterlimit-maxuserinput="15"
                                            data-val-characterlimit-minlength="5" data-val-characterlimit-onblur=""
                                            data-val-regex="Please enter valid characters only."
                                            data-val-regex-expression="^[a-zA-Z0-9]*$" data-val-regex-onblur=""
                                            data-val-regex-options="" id="BonusCode" name="about" spellcheck="false"
                                            type="text" style="width:331px;height:107px"><%=user.about%></textarea>

                                        <div></div>

                                        <input id="InformationText" name="InformationText" type="hidden" value="" />
                                    </div>
                                </div>


                            </div>
                        </div>
                    </fieldset>
                  

                    <fieldset data-section="submit">
                        <div>


                            <div data-control="button">
                                <div>
                                    <button type="button" id="Submit">Update</button>
                                </div>
                            </div>
                            <div data-control="creating-account">
                                <div>Creating Account</div>

                                <div></div>

                            </div>
                            <div data-control="account-created">
                                <div>Account Created</div>

                                <div></div>

                            </div>
                            <div data-control="cannot-create-account">

                                <div></div>

                                <div>Account Cannot Be Created</div>
                            </div>
                            <div data-control="unable-to-login">

                                <div></div>

                                <div>Unable to Login</div>
                            </div>
                        </div>
                        <input id="gdprSubmitClicked" type="hidden" value="false" />
                    </fieldset>


                </div>
                <div data-control="lightBox">
                    <div>
                        <div class="lightBoxHeader">
                            <div class="lightBoxCloseButton"></div>
                            <h1></h1>
                        </div>
                        <div class="lightBoxBody">
